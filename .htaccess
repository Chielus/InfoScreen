AddType text/cache-manifest .mf

AddDefaultCharset UTF-8

# URL Rewrite fu
Options +FollowSymLinks
Options -Indexes
<IfModule mod_rewrite.c>
RewriteEngine On
RewriteBase /
RewriteRule ^$ requesthandlers/controller.php?page=info [QSA,NC,L]
RewriteRule ^(\d*\.?\d*)/(\d*\.?\d*)/$ requesthandlers/controller.php?page=info&x=$2&y=$1 [QSA,NC,L]
</IfModule>

